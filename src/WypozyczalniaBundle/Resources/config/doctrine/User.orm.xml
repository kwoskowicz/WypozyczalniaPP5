<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="WypozyczalniaBundle\Entity\User" table="User">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="login" type="string" column="login" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telephoneNumber" type="string" column="telephone_number" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="role" type="smallint" column="role" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="integer" column="status" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
